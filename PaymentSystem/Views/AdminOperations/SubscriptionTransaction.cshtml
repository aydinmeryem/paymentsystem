@model PaymentSystem.Models.CreateDeleteSubscriberModel
@{
    ViewBag.Title = "SubscriptionTransaction";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h3>Abonelik İşlemleri</h3>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-6">
            <p>Birseysel Müşteri</p>
            <form>
                <div class="form-group">
                    <label for="tc_">TC Kimlik Numarası:</label>
                    <input class="form-control" type="text" id="tc_" name="tc_" />
                </div>
                <div class="form-group">
                    <input class="form-control" type="submit" id="searchConSub" formaction="@Url.Action("SubscriptionTransactionConsumerResult","AdminOperations")" />
                </div>
            </form>
        </div>
        <div class="col-lg-6">
            <p>Kurumsal Müşteri</p>
            <form>
                <div class="form-group">
                    <label for="tax">Vergi Numarası:</label>
                    <input class="form-control" type="text" id="tax_" name="tax_" />
                </div>
                <div class="form-group">
                    <input class="form-control" type="submit" id="searchEntSub" formaction="@Url.Action("SubscriptionTransactionEnterpriseResult","AdminOperations")" />
                </div>
            </form>
        </div>
    </div>
</div>
